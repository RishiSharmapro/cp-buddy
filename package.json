{
  "name": "cp-buddy",
  "displayName": "CP-buddy",
  "description": "helper extension for competitive programers on vs code ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cp-buddy.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "cp-buddy.runTestCases",
        "title": "Run Test Cases",
        "category": "Cp-buddy",
        "icon": {
          "light": "./button/web/light.png",
          "dark": "./dark.png"
        }
      }
    ],
    "keybindings": [
      {
        "command": "cp-buddy.runTestCases",
        "key": "ctrl+'",
        "mac": "cmd+'",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "cp-buddy.runTestCases",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "puppeteer": "^23.11.1"
  }
}
